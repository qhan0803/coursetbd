<!-- quick add -->

  <button type="submit" class = 'quick_add' on = '0' data-toggle="modal" data-target="#quick_add_modal"><i class="fa fa-calendar-plus-o fa-2x"></i></br>  Add Event </button>

  <!-- quick_add Modal -->
<div class="modal fade" id="quick_add_modal" tabindex="-1" role="dialog" aria-labelledby="quick_add_modalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="quick_add_modalLabel">Create Event</h4>
      </div>
      <div class="modal-body">
        <form class="form-horizontal">
          
        <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Title(*):</label>
                          <div class="col-sm-10">

            <input type="text" class="form-control personal_title" id="personal_title" maxlength="30">
            </div>
        </div>

        <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Date and Time(*): </label>
                          <div class="col-sm-7">

            <input type="datetime-local" class="form-control personal_date_and_time" id="personal_date_and_time" max="2029-12-31T23:59:59">
          </div>
          <div class = "col-sm-3">
          <div class="checkbox">
              <label><input type="checkbox" value="" class = "personal_repeat">Repeat Weekly</label>
          </div>
        </div>

  </div>


   <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Duration:</label>
              <div class="col-sm-10">

        <div class="input-group">
  <input type="number" min="0" max="500" class="form-control personal_duration" id="personal_duration" aria-describedby="basic-addon2" >
  <span class="input-group-addon" id="basic-addon2">minutes</span>
</div>
</div>
  </div>  

    <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Location:</label>
                          <div class="col-sm-10">

            <input type="text" class="form-control personal_location" id="personal_location">
          </div>
  </div>

  
          <div class="form-group">

            <label for="message-text" class="col-sm-2 control-label">Description:</label>
                          <div class="col-sm-10">

            <textarea class="form-control personal_description" id="message-text"></textarea>
          </div>
          </div>
        </form>
      </div>


      <div class="modal-footer">
          <div class = "error"> </div>

          <button type="button" class="btn btn-primary add_event">Add </button>
         

      </div>
    </div>
  </div>
</div>
	

	{{#if TA}}
	
  <button type="submit" class = 'ta_portal' on = '0' data-toggle="modal" data-target="#ta_portal_modal"><i class="fa fa-bullhorn fa-2x fa-flip-horizontal"></i></br>  TA Portal </button>

  <!-- Modal -->
