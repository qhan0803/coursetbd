          <option>Pset Due</option>
          <option>Office Hour</option>
        <option>Review Session</option>
          <option>Exam</option>
      </select>
    </div>
      </div>

    <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Location(*):</label>
