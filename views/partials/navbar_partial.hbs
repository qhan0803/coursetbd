            <input type="text" class="form-control ta_location" id="location">
          </div>
  </div>

  <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Date and Time(*): </label>
                          <div class="col-sm-7">

            <input type="datetime-local" class="form-control ta_date_and_time" id="date_and_time" max="2029-12-31T23:59:59">
          </div>
          <div class="col-sm-3">
           <div class="checkbox">
              <label><input type="checkbox" value="" class = "ta_repeat">Repeat Weekly</label>
          </div>
        </div>
  </div>
   <div class="form-group">
            <label for="recipient-name" class="col-sm-2 control-label">Duration:</label>
              <div class="col-sm-10">

        <div class="input-group">
  <input type="number" min="0" max="500" class="form-control ta_duration" id="duration" aria-describedby="basic-addon2" >
  <span class="input-group-addon" id="basic-addon2">minutes</span>
</div>
</div>
  </div>
          <div class="form-group">

            <label for="message-text" class="col-sm-2 control-label">Description:</label>
                          <div class="col-sm-10">

            <textarea class="form-control ta_description" id="message-text"></textarea>
          </div>
          </div>
        </form>
      </div>


      <div class="modal-footer">
          <div class = "error"> </div>

          <button type="button" class="btn btn-primary create_announcement">Add</button>
      </div>
    </div>
  </div>
</div>

